<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Raspberry_Pi_Trebuchet.UWP_UI.Desktop">

    <Style TargetType="local2:PiSettingsPanel" xmlns:local2="using:Raspberry_Pi_Trebuchet.UWP_UI.Desktop.Controls.SettingsPanel">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local2:PiSettingsPanel">
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
